#include <iostream>
using namespace std;

void garis(){
	cout << "\n\n------------------------------------\n";
}

void nilaiMahasiswa (int nilaiAkhir){
	
	string nilaiAkhirMahasiswa = "E";
	
	if (nilaiAkhir >= 75){
		nilaiAkhirMahasiswa = "A";
	}
	else if (nilaiAkhir >= 60){
		nilaiAkhirMahasiswa = "B";
	}
	else if (nilaiAkhir >= 45){
		nilaiAkhirMahasiswa = "C";
	}
	else if (nilaiAkhir < 45){
		nilaiAkhirMahasiswa = "D";
	}
	
	cout << "Hasil Akhir Anda adalah " << nilaiAkhirMahasiswa << " ";
}

int main(){
	nilaiMahasiswa (50);
	
	return 0;
}
